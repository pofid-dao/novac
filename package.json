{
  "private": true,
  "main": ".",
  "homepage": "./",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/test": "^3.0.14",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sero-pp": "file:../sero-pp",
    "serojs": "0.0.2",
    "umi": "^3.0.14",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/plugin-antd": "^0.5.0",
    "axios": "^0.19.2",
    "bignumber.js": "^9.0.0",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "i18next-http-backend": "^1.0.8",
    "i18next-localstorage-cache": "^1.1.1",
    "i18next-sprintf-postprocessor": "^0.2.2",
    "react-i18next": "^11.4.0"
  }
}
